class Coin {
	
	field int x, y;
	field int value;
	field int rotationCounter;
	field Box src;
	
	/** Constructs a new coin, given the Box object from which it came out. */
	constructor Coin new(Box source) {
		let x = source.getX();
		let y = source.getY();
		let rotationCounter = 0;
		let value = 10;
		let src = source;
		do jump();
		return this;
	}
	
	/** Returns the value of the coin. */
	method int getValue() {
		return value;
	}
	
	/** Disposes this coin. */

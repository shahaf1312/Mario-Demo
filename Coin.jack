   /** Moves the coin up by 3 pixels and changes its graphic represrentation so it will emulate a turning effect. */
	method void jump() {
		if (y = 0) {
			do Sys.wait(300);
			do erase();
			do MarioGame.setCoin();
			do dispose();
			return;
		}
		do erase();
		let y = y - 3;
		if ((y & 1) = 0) {
			let rotationCounter = rotationCounter + 1;
			if (rotationCounter > 5) {
				let rotationCounter = 0;
			}
		}
		
		do draw();
		do src.draw();
		return;
	}
